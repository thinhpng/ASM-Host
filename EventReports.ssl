	mPreviousRCState 		= S2_Decode(mPRCSList[0]);
	mRCStateReport 			= Decode(ReportList[2]);
	mRCSList 				= Decode(mRCStateReport[1]);
	mRCState 				= S2_Decode(mRCSList[0]);
