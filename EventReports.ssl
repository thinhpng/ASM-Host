GetRCStatus(RCn, ReportList){
	local mPreviousRCStateReport, mPreviousRCState, mRCStateReport, mPRCSList, mRCState, mRCSList; 
	local mRCStateChangeReport, mRCStateChangeRCPPName, mRCSCRCPPNList;
	//logn(">>>>>> GetRC", RCn, "Status <<<<<<<");
